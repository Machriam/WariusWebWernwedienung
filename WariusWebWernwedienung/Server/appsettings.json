{
  "Kestrel": {
    "Endpoints": {
      "Grpc": {
        "Url": "https://0.0.0.0:5000",
        "Protocols": "Http2"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "D:\\Certs\\wert.crt",
        "KeyPath": "D:\\Certs\\wert.key"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "ScriptFolder": "D:\\Scripts"
  },
  "ParameterReplacements": {
    "{Dec200Pass}": "Your Password"
  },
  "AllowedHosts": "*"
}